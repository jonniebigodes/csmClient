{
  "name": "csmClient",
  "version": "1.0.0",
  "description": "implementacao cliente csm",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debugserver": "nodemon server.js --exec babel-node",
    "start": "node ./lib/server.bundle.js",
    "watch": "webpack -d --watch",
    "builddev": "webpack --config webpack.config.js --progress --colors",
    "buildserver": "babel server.js -o ./lib/server.bundle.js --presets minify",
    "postinstall": "npm run build && webpack -p --config webpack.prod.config.js --progress --colors",
    "build": "npm run buildserver",
    "showerrors": "webpack --progress --color --watch --display-error-details",
    "showinfo": "webpack --display-modules --display-reasons",
    "showinfofile": "webpack --json>stats.json",
    "showfilesizefile": "webpack --display-modules --display-reasons --sort-modules-by size --json>stats.json",
    "spiterror": "webpack --display-error-details"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonniebigodes/csmClient.git"
  },
  "keywords": [
    "heroku",
    "dynamic apps",
    "csm"
  ],
  "author": "joao cardoso l17719",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonniebigodes/csmClient/issues"
  },
  "homepage": "https://github.com/jonniebigodes/csmClient#readme",
  "dependencies": {
    "animate.css": "^3.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "css-loader": "0.23.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.15.3",
    "file-loader": "^0.10.0",
    "material-ui": "^0.20.0",
    "moment": "^2.19.0",
    "node-sass": "^3.4.2",
    "normalizr": "^3.2.4",
    "prop-types": "^15.5.10",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.4",
    "clean-webpack-plugin": "^0.1.15",
    "cross-env": "^5.0.5",
    "eslint": "^3.19.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.11.0",
    "redux-devtools": "^3.3.2",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
